    {
      "cell_type": "code",
      "source": [
        "from sklearn.neighbors import BallTree\n",
        "from sklearn.preprocessing import StandardScaler\n",
        "from sklearn.ensemble import RandomForestRegressor, RandomForestClassifier\n",
        "\n",
        "#import data kaggle\n",
        "df = pd.read_csv(\"pharmacies_in_west_bengal.csv") #ganti path ke data csv kaggle\n",
        "\n",
        "df = df.rename(columns={\n",
        "    \"Latitude\": \"latitude\",\n",
        "    \"Longitude\": \"longitude\",\n",
        "    \"lat\": \"latitude\",\n",
        "    \"lon\": \"longitude\"\n",
        "})\n",
        "\n",
        "df = df[['latitude', 'longitude']].dropna().reset_index(drop=True)\n",
        "\n",
        "#import data populasi\n",
        "raster = rasterio.open(\"ind_ppp_2020.tif")\n",
        "\n",
        "def get_population(lat, lon):\n",
        "    try:\n",
        "        row, col = raster.index(lon, lat)\n",
        "        value = raster.read(1)[row, col]\n",
        "        return max(value, 0)\n",
        "    except:\n",
        "        return 0\n",
        "\n",
        "df[\"population\"] = df.apply(\n",
        "    lambda r: get_population(r.latitude, r.longitude),\n",
        "    axis=1\n",
        ")\n"
      ],
      "metadata": {
        "id": "82IpIYaWn5Ta"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#menghitung apotek dalam radius\n",
        "coords = np.radians(df[['latitude', 'longitude']].values)\n",
        "tree = BallTree(coords, metric=\"haversine\")\n",
        "\n",
        "R_EARTH = 6371  # km\n",
        "r5 = 5 / R_EARTH\n",
        "r10 = 10 / R_EARTH\n",
        "\n",
        "df[\"pharmacy_5km\"] = tree.query_radius(coords, r=r5, count_only=True)\n",
        "df[\"pharmacy_10km\"] = tree.query_radius(coords, r=r10, count_only=True)"
      ],
      "metadata": {
        "id": "KtlcOkWvn_TV"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#menghitung kepdatan penduduk\n",
        "area_5km = np.pi * (5 ** 2)  # km^2\n",
        "df[\"pop_density_5km\"] = df[\"population\"] / area_5km"
      ],
      "metadata": {
        "id": "NDHXXrWnoDkC"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#membuat regresi\n",
        "df[\"need_score\"] = df[\"pop_density_5km\"] / (df[\"pharmacy_5km\"] + 1)"
      ],
      "metadata": {
        "id": "ZuA6D1-goIgu"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#training\n",
        "features = [\"pop_density_5km\", \"pharmacy_5km\", \"pharmacy_10km\"]\n",
        "X = df[features]\n",
        "y = df[\"need_score\"]\n",
        "\n",
        "scaler = StandardScaler()\n",
        "X_scaled = scaler.fit_transform(X)\n",
        "\n",
        "reg_model = RandomForestRegressor(\n",
        "    n_estimators=300,\n",
        "    max_depth=10,\n",
        "    random_state=42\n",
        ")\n",
        "reg_model.fit(X_scaled, y)\n",
        "\n",
        "df[\"predicted_need\"] = reg_model.predict(X_scaled)"
      ],
      "metadata": {
        "id": "ewt_9vUnoOcx"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#melakukan klasifikasi\n",
        "threshold = df[\"predicted_need\"].quantile(0.7)\n",
        "df[\"priority_label\"] = (df[\"predicted_need\"] >= threshold).astype(int)"
      ],
      "metadata": {
        "id": "j_PJl3L8oRrJ"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#train model klasifikasi\n",
        "cls_model = RandomForestClassifier(\n",
        "    n_estimators=300,\n",
        "    max_depth=10,\n",
        "    random_state=42\n",
        ")\n",
        "cls_model.fit(X_scaled, df[\"priority_label\"])"
      ],
      "metadata": {
        "id": "SP66VCbuoVM6"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#menyimpan model\n",
        "joblib.dump(scaler, \"scaler.pkl\")\n",
        "joblib.dump(reg_model, \"regression_model.pkl\")\n",
        "joblib.dump(cls_model, \"classification_model.pkl\")\n",
        "joblib.dump(threshold, \"priority_threshold.pkl\")\n",
        "print(\"Model Telah Disimpan\")\n",
        "\n"
      ],
      "metadata": {
        "id": "sgz-_8GboZAt"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "import folium\n",
        "from folium.plugins import HeatMap"
      ],
      "metadata": {
        "id": "YddvwUkp8eA_"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "df['need_norm'] = (\n",
        "    df['predicted_need'] - df['predicted_need'].min()\n",
        ") / (\n",
        "    df['predicted_need'].max() - df['predicted_need'].min()\n",
        ")\n",
        "\n",
        "#base map\n",
        "center_lat = df['latitude'].mean()\n",
        "center_lon = df['longitude'].mean()\n",
        "\n",
        "m = folium.Map(\n",
        "    location=[center_lat, center_lon],\n",
        "    zoom_start=6,\n",
        "    tiles=\"CartoDB positron\"\n",
        ")\n",
        "\n",
        "# Heatmap (REGRESI)\n",
        "heat_data = df[['latitude', 'longitude', 'need_norm']].values.tolist()\n",
        "\n",
        "HeatMap(\n",
        "    heat_data,\n",
        "    radius=15,\n",
        "    blur=20,\n",
        "    min_opacity=0.4,\n",
        "    name=\"Predicted Need (Regression)\"\n",
        ").add_to(m)\n",
        "\n",
        "# Marker (KLASIFIKASI)\n",
        "priority_layer = folium.FeatureGroup(name=\"Priority Classification\")\n",
        "\n",
        "for _, row in df.iterrows():\n",
        "    if row['priority_label'] == 1:\n",
        "        color = \"red\"\n",
        "        status = \"PRIORITY\"\n",
        "    else:\n",
        "        color = \"green\"\n",
        "        status = \"NON-PRIORITY\"\n",
        "\n",
        "    folium.CircleMarker(\n",
        "        location=[row['latitude'], row['longitude']],\n",
        "        radius=4,\n",
        "        color=color,\n",
        "        fill=True,\n",
        "        fill_color=color,\n",
        "        fill_opacity=0.7,\n",
        "        popup=folium.Popup(\n",
        "            f\"\"\"\n",
        "            <b>Status:</b> {status}<br>\n",
        "            <b>Predicted Need:</b> {row['predicted_need']:.2f}<br>\n",
        "            <b>Population Density (5km):</b> {row['pop_density_5km']:.1f}<br>\n",
        "            <b>Pharmacy 5km:</b> {row['pharmacy_5km']}\n",
        "            \"\"\",\n",
        "            max_width=300\n",
        "        )\n",
        "    ).add_to(priority_layer)\n",
        "\n",
        "priority_layer.add_to(m)\n",
        "\n",
        "# layer control\n",
        "folium.LayerControl(collapsed=False).add_to(m)\n",
        "\n",
        "# save map\n",
        "m.save(\"healthcare_priority_map.html\")\n",
        "\n",
        "print(\"âœ… SELESAI\")\n",
        "print(\"ðŸ“ Output map: healthcare_priority_map.html\")\n"
      ],
      "metadata": {
        "id": "gOB7Y7Fx8kUW"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#Load Model\n",
        "scaler = joblib.load(\"scaler.pkl\")\n",
        "reg_model = joblib.load(\"regression_model.pkl\")\n",
        "cls_model = joblib.load(\"classification_model.pkl\")\n",
        "threshold = joblib.load(\"priority_threshold.pkl\")\n",
        "print(\"Load Model\")"
      ],
      "metadata": {
        "id": "wciP2_TO8hGH"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#prediksi baru\n",
        "new_data = pd.DataFrame([\n",
        "    {\n",
        "        \"pop_density_5km\": 1500,\n",
        "        \"pharmacy_5km\": 1,\n",
        "        \"pharmacy_10km\": 4\n",
        "    },\n",
        "    {\n",
        "        \"pop_density_5km\": 300,\n",
        "        \"pharmacy_5km\": 6,\n",
        "        \"pharmacy_10km\": 15\n",
        "    }\n",
        "])\n",
        "\n",
        "X_new_scaled = scaler.transform(new_data)"
      ],
      "metadata": {
        "id": "bj45o6uqoj2O"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "#regresi baru\n",
        "new_data[\"predicted_need\"] = reg_model.predict(X_new_scaled)\n",
        "\n",
        "#klasifikasi baru\n",
        "new_data[\"priority_class\"] = (\n",
        "    new_data[\"predicted_need\"] >= threshold\n",
        ").astype(int)\n",
        "\n",
        "print(\"\\n HASIL PREDIKSI:\")\n",
        "print(new_data)"
      ],
      "metadata": {
        "id": "l_1UpXM2on-5"
      },
